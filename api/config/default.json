{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "secret": "",
    "path": "/authentication",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://localhost:3030",
      "algorithm": "HS256",
      "expiresIn": "1d"
    }
  },
  "mailer": {
    "message": {
      "from": "kontakt@mail.ernte-teilen.org",
      "replyTo": "kontakt@rernte-teilen.org"
    },
    "transport": {
      "jsonTransport": true
    }
  },
  "search": {
    "app_id": "GEOCODER_APP_ID",
    "app_code": "GEOCODER_APP_CODE"
  },
  "autocomplete": {
    "country": "DEU,AUT,CHE,LIE",
    "language": "de"
  },
  "postgres": {
    "client": "pg",
    "connection": "postgresql://teikei:teikei@localhost:5432/teikei_dev"
  },
  "errorhandler": {
    "html": false
  },
  "redis": {
    "url": "redis://localhost:6379"
  },
  "enableJobQueues": false
}
