{
  "type": "object",
  "title": "Depot Properties",
  "required": [
    "name",
    "city",
    "address",
    "latitude",
    "longitude"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string",
      "maxLength": 255
    },
    "city": {
      "type": "string",
      "maxLength": 255
    },
    "address": {
      "type": "string",
      "maxLength": 255
    },
    "latitude": {
      "type": "number"
    },
    "longitude": {
      "type": "number"
    },
    "url": {
      "anyOf": [
        {
          "type": "string",
          "maxLength": 255,
          "format": "url"
        },
        {
          "type": "string",
          "enum": [
            ""
          ]
        }
      ]
    },
    "description": {
      "type": "string"
    },
    "delivery_days": {
      "type": "string",
      "maxLength": 255
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
