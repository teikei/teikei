{
  "name": "@teikei/admin",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x",
    "npm": "6.x",
    "yarn": "1.x"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "date-fns": "2.23.0",
    "feathers-client": "2.4.0",
    "jwt-decode": "3.1.2",
    "prop-types": "15.7.2",
    "ra-data-feathers": "2.9.1",
    "ra-data-json-server": "3.17.2",
    "react": "17.0.2",
    "react-admin": "3.17.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "recharts": "2.1.2",
    "superagent": "6.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.0",
    "@mdx-js/loader": "^1.6.22",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-jsx": "10.0.0",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-standard": "5.0.0",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "standard": "16.0.3"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true FORCE_COLOR=true react-scripts start | cat -",
    "dev": "npm run start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "clean": "rm -rf build && mkdir build",
    "test": "SKIP_PREFLIGHT_CHECK=true echo 'noop'",
    "eject": "react-scripts eject",
    "lint": "npx eslint . --fix",
    "pretty-quick": "npx pretty-quick --staged",
    "prettier": "npx prettier '**/*.js' --write"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "ignorePatterns": [
      "build/**"
    ],
    "rules": {
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0,
      "spaced-comment": 0,
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "no-console": 0
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "jest/globals": true,
      "browser": true
    },
    "plugins": [
      "jest"
    ],
    "extends": [
      "standard",
      "standard-jsx",
      "standard-react",
      "plugin:cypress/recommended",
      "prettier"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
